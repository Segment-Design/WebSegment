#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Development/projects/WebSegment/packages/@websegment-cli/dist/node_modules:/Volumes/Development/projects/WebSegment/packages/@websegment-cli/node_modules:/Volumes/Development/projects/WebSegment/packages/node_modules:/Volumes/Development/projects/WebSegment/node_modules:/Volumes/Development/projects/node_modules:/Volumes/Development/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Development/projects/WebSegment/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Development/projects/WebSegment/packages/@websegment-cli/dist/node_modules:/Volumes/Development/projects/WebSegment/packages/@websegment-cli/node_modules:/Volumes/Development/projects/WebSegment/packages/node_modules:/Volumes/Development/projects/WebSegment/node_modules:/Volumes/Development/projects/node_modules:/Volumes/Development/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Development/projects/WebSegment/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@websegment/cli/dist/index.mjs" "$@"
else
  exec node  "$basedir/../@websegment/cli/dist/index.mjs" "$@"
fi
